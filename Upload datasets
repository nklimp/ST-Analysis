library(STutility)

#upload files
infoTable <- read.table("~/Desktop/MOB_164315_infoTable.csv", sep=",", header=T, stringsAsFactors = F)

#Filter dataset and generate Seurat object. All filter values set to 1 for OR analysis
se <- InputFromTable(infotable = infoTable, 
                      min.gene.count = 1, 
                      min.gene.spots = 1,
                      min.spot.count = 1,
                      platform =  "Visium")
